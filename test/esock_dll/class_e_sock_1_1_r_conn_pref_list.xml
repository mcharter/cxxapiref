		<cxxClassNestedDetail>
			<cxxClassNestedClass href="class_e_sock_1_1_r_conn_pref_list_1_1_t_iter.xml#class_e_sock_1_1_r_conn_pref_list_1_1_t_iter">ESock::RConnPrefList::TIter</cxxClassNestedClass>
