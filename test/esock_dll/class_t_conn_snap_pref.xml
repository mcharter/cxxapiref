			</cxxClassDerivations>
			<cxxClassAPIItemLocation>
				<cxxClassDeclarationFile name="filePath" value="K:/sf/os/commsfw/datacommsserver/esockserver/inc/connpref.h"/>
				<cxxClassDeclarationFileLine name="lineNumber" value="120"/>
