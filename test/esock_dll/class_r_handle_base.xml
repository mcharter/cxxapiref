	<cxxFunction id="class_r_handle_base_1a856c39af65967538e388a65fac4c5b0c">
		<apiName>SetReturnedHandle</apiName>
		<cxxFunctionDetail>
			<cxxFunctionDefinition>
				<cxxFunctionAccessSpecifier value="public"/>
				<cxxFunctionInline/>
				<cxxFunctionDeclaredType>
					<apiRelation href="e32def_8h.xml#_1abb88f5378e8305d934297176fe5fa298">TInt</apiRelation>
				</cxxFunctionDeclaredType>
				<cxxFunctionScopedName>RHandleBase</cxxFunctionScopedName>
				<cxxFunctionPrototype>inline TInt SetReturnedHandle(TInt aHandleOrError)</cxxFunctionPrototype>
				<cxxFunctionNameLookup>RHandleBase::SetReturnedHandle(TInt)</cxxFunctionNameLookup>
				<cxxFunctionParameters>
					<cxxFunctionParameter>
						<cxxFunctionParameterDeclaredType>
							<apiRelation href="e32def_8h.xml#_1abb88f5378e8305d934297176fe5fa298">TInt</apiRelation>
						</cxxFunctionParameterDeclaredType>
						<cxxFunctionParameterDeclarationName>aHandleOrError</cxxFunctionParameterDeclarationName>
						<apiDefNote>A handle-number, if zero or positive; an error value, if negative.</apiDefNote>
					</cxxFunctionParameter>
				</cxxFunctionParameters>
				<cxxFunctionAPIItemLocation>
					<cxxFunctionDeclarationFile name="filePath" value="K:/epoc32/include/e32cmn.h"/>
					<cxxFunctionDeclarationFileLine name="lineNumber" value="2415"/>
					<cxxFunctionDefinitionFile name="filePath" value="K:/sf/os/commsfw/datacommsserver/esockserver/csock/CS_CLI.CPP"/>
					<cxxFunctionDefinitionFileLineStart name="lineNumber" value="3152"/>
					<cxxFunctionDefinitionFileLineEnd name="lineNumber" value="3177"/>
				</cxxFunctionAPIItemLocation>
			</cxxFunctionDefinition>
			<apiDesc>
				<p>Sets the handle-number of this handle to the specified value.</p>
				<p>The function can take a (zero or positive) handle-number, or a (negative) error number.</p>
				<p>If aHandleOrError represents a handle-number, then the handle-number of this handle is set to that value. If aHandleOrError represents an error number, then the handle-number of this handle is set to zero and the negative value is returned.</p>
				<p>KErrNone, if aHandle is a handle-number; the value of aHandleOrError, otherwise. </p>
			</apiDesc>
		</cxxFunctionDetail>
	</cxxFunction>
