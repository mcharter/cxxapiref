    <xs:choice>
      <xs:element ref="topicref"/>
      <xs:element ref="mapgroup-d-topicref"/>
    </xs:choice>
  </xs:group>
  <!-- vocabulary attributes (must be declared ahead of the dtds, which puts @domains first in order) -->
