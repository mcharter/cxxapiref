      <xs:attribute name="class" default="- topic/state reference/state apiRef/apiQualifier apiClassifier/apiQualifier cxxClass/cxxClassAccessSpecifier "/>
    </xs:complexType>
  </xs:element>
  <xs:element name="cxxClassAbstract">
    <xs:complexType>
