        <section conref="../packagedef.dita#cxxUnionDeclarationFileLineStart-reference/cxxUnionDeclarationFileLineStart-contains-section"/>
        <section conref="../packagedef.dita#cxxUnionDeclarationFileLineStart-reference/cxxUnionDeclarationFileLineStart-contentModel-section"/>
